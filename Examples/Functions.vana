function Add(double a, double b) :: double {
    return a + b;
}

function Multiply(double a, double b) :: double {
    return a * b;
}

function Main() {
    double x = 5;
    double y = 10;
    
    double sum = Add(x, y);
    double product = Multiply(x, y);
    
    print(sum);
    print(product);
}
