// VANARIZE TEST: Control Flow (If-Else, While Loop)
// Master Plan Compliance: Typed Variables, C-style loops

function Main() {
    number counter = 0;
    number max = 5;
    
    while (counter < max) {
        if (counter == 3) {
            print(999);
        } else {
            print(counter);
        }
        counter = counter + 1;
    }
}
