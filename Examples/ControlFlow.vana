// VANARIZE TEST: Control Flow (If-Else, While Loop)
// Master Plan Compliance: Typed Variables, C-style loops

function Main() {
    number sum = 0;
    number max = 5;
    for (number counter = 0; counter < max; counter = counter + 1) {
        if (counter == 3) {
            sum = sum + 999;
        } else {
            sum = sum + counter;
        }
    }
    print("Sum: " + sum);
}
